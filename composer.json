{
    "name": "phpatom/di",
    "description": "An expressive and flexible dependency injection container for PHP",
    "keywords": ["atom", "di","dependency injection","container","dependency injection container","framework","ioc","inversion of control","psr-11"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4|8.*",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^9.2"
    },
    "authors": [
        {
            "name": "Nghokeng daniel",
            "email": "nghokend@gmail.com"
        }
    ],
    "scripts": {
        "test": "phpunit --coverage-clover=coverage.xml"
    },
    "autoload": {
        "psr-4": {
            "Atom\\DI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atom\\DI\\Tests\\": "tests"
        },
        "files": ["tests/Misc/functions.php"]
    }
}
